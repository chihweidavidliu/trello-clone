{
  "name": "server",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "yarn docker:up && yarn migrate-test && yarn seed-test && dotenv -e .env.test -- jest -i --detectOpenHandles",
    "tsc": "rm -rf ./build && tsc",
    "dev": "dotenv -e .env -- ts-node-dev ./src/index.ts",
    "build": "tsc",
    "start": "dotenv -e .env -- node ./build/index.js",
    "check-types": "tsc --noEmit",
    "build-types": "dotenv -e .env -- ts-node-dev ./src/db/kanel.ts",
    "create-migration": "yarn knex migrate:make --knexfile ./src/db/knexfile.ts",
    "knex": "dotenv -e .env -- knex",
    "knex-test": "dotenv -e .env.test -- knex",
    "_migrate": "knex migrate:latest --knexfile ./src/db/knexfile.ts",
    "migrate": "dotenv -e .env -- yarn _migrate",
    "migrate-test": "dotenv -e .env.test -- yarn _migrate",
    "_rollback": "knex migrate:rollback --knexfile ./src/db/knexfile.ts",
    "rollback": "dotenv -e .env -- yarn _rollback",
    "rollback-test": "dotenv -e .env.test -- yarn _rollback",
    "create-seed": "yarn knex seed:make --knexfile ./src/db/knexfile.ts",
    "_seed": "knex seed:run --knexfile ./src/db/knexfile.ts",
    "seed": "dotenv -e .env -- yarn _seed",
    "seed-test": "dotenv -e .env.test -- yarn _seed",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down"
  },
  "prisma": {
    "schema": "./src/prisma/schema.prisma",
    "seed": "ts-node-dev -r dotenv/config ./src/prisma/seed.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@types/cors": "^2.8.14",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "express-async-errors": "^3.1.1",
    "knex": "^2.5.1",
    "lodash": "^4.17.21",
    "pg": "^8.11.3",
    "shared-utils": "^1.0.0",
    "uuid": "^9.0.0",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.3",
    "@types/lodash": "^4.14.197",
    "@types/pg": "^8.10.2",
    "@types/supertest": "^2.0.12",
    "@types/uuid": "^9.0.3",
    "dotenv-cli": "^7.2.1",
    "jest": "^29.6.2",
    "kanel": "^3.5.3",
    "kanel-knex": "^2.3.1",
    "prisma": "^5.1.1",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-node-dev": "^2.0.0"
  }
}
